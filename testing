            <li >
            <img class="titleImg" width="40px" height="40px" src="https://cdn.discordapp.com/avatars/902306328427499611/a72daeb1d04e7eb0e3a8027c85962f35.webp" alt="">
            <div class="titleInfo">
                <p class="timeInfo"><span class="chatName">Admonel</span><span class="time">Sat Feb 25 2023 22:42:58 GMT-0600 (Central Standard Time)</span></p>
                <p><span style="color: skyblue;">@verif_design#3231</span> This should work:                                                                                                                                                                                                                                                                  program my_prog;
class my_class;
  
  rand int unsigned Arr [9][3][3];
  
  rand int max_val[9];
  int Final_Array[9][9];
  
  constraint uniq_max{
    foreach(max_val[i]) max_val[i] inside {[1:100]};
    unique {max_val};
    unique {Arr}; //This is not required, though
  }
  
  constraint sub_matrix {
    foreach(Arr[i,j,k])
      Arr[i][j][k] < max_val[i];
  
  }
 
  
  function void post_randomize();
    foreach(Arr[i])
      $display("%p", Arr[i]);

    for(int i=0; i<3; i++)
      for(int j=0; j<3; j++)
        Final_Array[3*i+j] = { Arr[3*i][j],Arr[3*i+1][j], Arr[3*i+2][j]};
  endfunction
endclass
  
  initial begin
    my_class K=new();
    repeat(1) begin
      K.randomize();
      for (int i=0; i< 9; i++)
          $display("%p ", K.Final_Array[i]);
    end
  end</p>
                
            </div>
        </li>
              
