            <li >
            <img class="titleImg" width="40px" height="40px" src="https://static-global.zingfront.com/extension/avator.png" alt="">
            <div class="titleInfo">
                <p class="timeInfo"><span class="chatName">HWMan</span><span class="time">Thu Mar 02 2023 03:06:53 GMT-0600 (Central Standard Time)</span></p>
                <p><span style="color: skyblue;">@dve#7219</span> thanks <@879610288503136266> 

`module top();
  
  bit [7:0] addr;
  
  bit clk;
  always #5 clk=!clk;
  
  covergroup g1 (ref bit[3:0] address); 
    option.per_instance = 1;
    addr_cp : coverpoint address {
      bins bin_0_A[] = {4'hA,4'h5}; 
    }
  endgroup
  
  g1 g1_inst[2];

    function void sample();
      $display("%h",addr);
      foreach(g1_inst[i]) g1_inst[i].sample();
    endfunction
  
  initial begin
    foreach(g1_inst[i]) g1_inst[i] =  new(addr[4*i+:4]);
    begin
      addr = 'h55;
      sample;
      @(posedge clk);
      addr = 'h5A;
      sample;
      @(posedge clk);
      addr = 'hA5;
      sample;
      @(posedge clk);
      addr = 'hAA;
      sample;
      @(posedge clk);
    end
    #10;
    $finish();
  end
  
  
  final
  begin
    $display("overall coverage = %0f", $get_coverage());
    foreach(g1_inst[i]) begin
        $display("coverage of covergroup cg_inst = %0f", g1_inst[i].get_coverage());
        $display("coverage of coverpoint CP1 = %0f", g1_inst[i].addr_cp.get_coverage());
    end
  end
  
endmodule`</p>
                
            </div>
        </li>
              
